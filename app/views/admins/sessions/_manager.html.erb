<% if current_admin %>  
  <li><%= link_to "Profile", profile_path, class:"dropdown-item" %></li>
  <li><hr class="h-px border-0 dark:bg-sky-900"></li>
  <li><%= button_to "Logout", destroy_admin_session_path, method: :delete, class:"dropdown-item" %></li>
<% elsif current_user %>
  <li><%= link_to "Profile", profile_path, class:"dropdown-item" %></li>
  <li><hr class="h-px border-0 dark:bg-sky-900"></li>
  <li><%= button_to "Logout", destroy_user_session_path, method: :delete, class:"nav-item text-2xl text-sky-600" %></li>
<% else %>
  <li class="nav-item text-2xl text-sky-600">
    <%= link_to "Register", new_user_registration_path %>
  </li>
  <li><hr class="h-px border-0 dark:bg-sky-900"></li>
  <li class="nav-item text-2xl text-sky-600">
    <%= link_to "Login", new_user_session_path %>
  </li>
<% end %>
